package com.bms.security.resource;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class EmployeeResource {
	
	@GetMapping("/")
	public String greet()
	{
		return "working";
	}
	
	@GetMapping
	public String greetAdmin("/admin")
	{
		return "Admin@work";
	}
	
	@GetMapping
	public String greetUser("/admin")
	{
		return "User@work";
	}
	
}
